    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
    <script>
      $(document).ready(function () {
        $("#table_id").DataTable();
      });
    </script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="//cdn.datatables.net/1.10.7/js/jquery.dataTables.min.js"></script>
    <title>Document</title>
  </head>
  <nav class="navbar navbar-expand-lg  nav-bg-blue fixed-top">
    <div class="container-fluid">
    
      <button class="navbar-toggler navbar-dark"  type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon navbar-dark"></span>
      </button>
      <div class="nav collapse navbar-collapse justify-content-end" id="navbarSupportedContent">
        <ul class="navbar-nav  mb-2 mb-lg-0 mx-2 navbarlist w-25 justify-content-evenly">
          <li class="nav-item">
            <a  class="nav-link main-font" id="home" href="/" >Help</a>
          </li>
         
          <li class="nav-item" >
            <a class="nav-link main-font" id="apply" href="/application.html">home</a>
          </li>
          <li class="nav-item " >
            <a class="nav-link main-font" id="status" href="/sign up page.html">Sign up</a>
          </li>
          
        </div>
      </div>
</nav>
      <div class="container mt-5">
        <table id="table_id" class="display">
          <thead>
            <tr>
                    <th>Name</th>
                    <th>email</th>
                    <th>address</th>
                    <!-- <th>Job ID</th> -->
                    <th>status</th>
            </tr>
          </thead>

          <tbody>
            <% if(bdata.length){ bdata.forEach(function(data){ %>
            <tr>
                <td><%=data.name %></td>
                    <td><%=data.email %></td>
                    <td><%=data.address %></td>
                    <!-- <td><%=data.jobid %></td> -->
                    <td>
                      <% if(data.Status){%> selected<%}else{%>Rejected<%}%>%>
                    </td>
            </tr>

            <% }) } else{ %>
            <h4>No data available</h4>
            <%}%>
          </tbody>
        </table>
      </div>
    </div>
